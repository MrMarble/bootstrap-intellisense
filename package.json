{
  "name": "bootstrap-intellisense",
  "displayName": "Bootstrap IntelliSense",
  "version": "1.1.10",
  "description": "Bootstrap IntelliSense: Enhance Bootstrap development with CSS class autocompletion for efficient coding in Visual Studio Code.",
  "categories": [
    "Linters"
  ],
  "keywords": [
    "bootstrap",
    "bootstrap5",
    "css",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmarble/bootstrap-intellisense"
  },
  "license": "MIT",
  "publisher": "mrmarble",
  "main": "./dist/extension.js",
  "scripts": {
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "vscode:prepublish": "pnpm run esbuild-base --minify",
    "clean": "rimraf dist",
    "check": "tsc --noEmit",
    "build": "pnpm run check && pnpm run clean && pnpm run esbuild-base --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "pnpm run esbuild-base --sourcemap",
    "esbuild-watch": "pnpm run esbuild-base --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint .",
    "test": "node ./src/test/runTest.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "bootstrap-intellisense.enable",
        "title": "Bootstrap IntelliSense: Enable"
      },
      {
        "command": "bootstrap-intellisense.changeVersion",
        "title": "Bootstrap IntelliSense: Change Version"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Bootstrap IntelliSense",
      "properties": {
        "bootstrapIntelliSense.version": {
          "type": "string",
          "default": "Bootstrap v5.3",
          "description": "Bootstrap versions",
          "enum": [
            "Bootstrap v5.3",
            "Bootstrap v5.2",
            "Bootstrap v5.1",
            "Bootstrap v5.0",
            "Bootstrap v4.6",
            "Bootstrap v4.5",
            "Bootstrap v4.4",
            "Bootstrap v4.3",
            "Bootstrap v4.2",
            "Bootstrap v4.1",
            "Bootstrap v4.0"
          ]
        }
      }
    },
    "icons": {
      "bootstrap-icon": {
        "description": "Bootstrap Icon",
        "default": {
          "fontPath": "./assets/bootstrap-icon-font.woff",
          "fontCharacter": "B"
        }
      }
    }
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/node-fetch": "^2.6.11",
    "@types/vscode": "^1.67.1",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vscode/test-electron": "^2.2.2",
    "@vscode/vsce": "^2.26.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "engines": {
    "vscode": "^1.67.1"
  },
  "icon": "assets/icons/icon.png",
  "configurationDefaults": {
    "bootstrapIntelliSense.version": "Bootstrap v5.3"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/mrmarble"
  }
}
